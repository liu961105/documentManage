<?xml version="1.0" encoding="UTF8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lzn.document.documentmanage.mapper.file.FileMapper">
    <select id="selectFileById" parameterType="string" resultType="com.lzn.document.documentmanage.mode.file.Files">
        select * from file where id=#{id};
    </select>
</mapper>